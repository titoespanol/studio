{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appHosting": {
      "backends": [
        {
          "id": "studio",
          "region": "us-central1"
        }
      ]
    },
    "rewrites": [
      {
        "source": "**",
        "appHosting": {
          "backendId": "studio"
        }
      }
    ]
  }
}
