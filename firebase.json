{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appHosting": {
      "backends": [
        {
          "id": "studio",
          "region": "us-central1"
        }
      ]
    },
    "redirects": [
      {
        "source": "/www.thechildlens.com",
        "destination": "https://thechildlens.com",
        "type": 301
      }
    ]
  }
}
